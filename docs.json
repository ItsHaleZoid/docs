{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Asimov",
  "colors": {
    "primary": "#000000",
    "light": "#ffffff",
    "dark": "#000000"
  },
  "background": {
    "color": {
      "light": "#ffffff",
      "dark": "#000000"
    }
  },
  "favicon": "/favicon.svg",
  "api": {
    "mdx": {
      "server": "https://asimov.mov",
      "auth": {
        "method": "bearer",
        "name": "Authorization"
      }
    },
    "playground": {
      "display": "interactive",
      "proxy": true
    },
    "examples": {
      "languages": ["curl", "javascript", "python"],
      "defaults": "required"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Welcome",
        "icon": "square",
        "pages": [
          "welcome"
        ]
      },
      {
        "tab": "Documentation",
        "icon": "book",
        "groups": [
          {
            "group": "Console",
            "pages": [
              "main",
              "api",
              "graph"
            ]
          }
      
         
       
        ]
      },
      {
        "tab": "SDKs",
        "icon": "code",
        
        "groups": [

          {
            "group": "Introduction",
            "pages": [
              "sdk/overview"
            ]
          },
          
          {
            "group": "Lithio SDKs",
            "pages": [
              "sdk/python",
              "sdk/typescript"
            ]
          }

          

        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://console.lithio.dev",
          "icon": "home"
        },
        {
          "anchor": "Email",
          "href": "mailto:support@lithio.dev",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/lithio-docs-logo-black.png",
    "dark": "/logo/lithio-docs-logo-white.png",
    "dimensions": {
      "width": 1000,
      "height": 1000
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@lithio.dev"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://lithio.dev/dashboard"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "chatgpt",
      "claude",
      "view",
      "perplexity",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/lithioai"
    },
    "links": [
      {
        "header": "Product",
        "items": [
          {
            "label": "Features",
            "href": "https://lithio.dev/features"
          },
          {
            "label": "Pricing",
            "href": "https://lithio.dev/pricing"
          },
          {
            "label": "About",
            "href": "https://lithio.dev/about"
          }
        ]
      },
      {
        "header": "Company",
        "items": [
          {
            "label": "FAQs",
            "href": "https://lithio.dev/faq"
          },
          {
            "label": "Privacy Policy",
            "href": "https://lithio.dev/privacy"
          },
          {
            "label": "Terms of Service",
            "href": "https://lithio.dev/tos"
          }
      
        ]
      }
    ]
  }
}